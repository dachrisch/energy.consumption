{
  "name": "vite-template-solid",
  "version": "3.8.7",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "node scripts/startMemoryMongo.cjs",
    "vite-dev": "vite --host",
    "build": "vite build && npm run build:server",
    "build:server": "esbuild src/api/server.ts --bundle --platform=node --outfile=dist-server/server.js --format=esm --packages=external",
    "start:prod": "node dist-server/server.js",
    "serve": "vite preview",
    "test": "vitest --run",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx -c .eslintrc.quality.json",
    "release": "standard-version && git push --follow-tags origin HEAD",
    "release:patch": "standard-version --release-as patch && git push --follow-tags origin HEAD",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin HEAD",
    "release:major": "standard-version --release-as major && git push --follow-tags origin HEAD"
  },
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "4.0.18",
    "concurrently": "^9.2.1",
    "daisyui": "^5.5.14",
    "esbuild": "^0.27.2",
    "eslint": "^8.57.1",
    "jsdom": "^27.4.0",
    "mongodb-memory-server": "^11.0.1",
    "solid-devtools": "^0.34.3",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2",
    "vite": "^7.1.4",
    "vite-plugin-solid": "^2.11.8",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@solidjs/router": "^0.15.4",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.2",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.5",
    "solid-chartjs": "^1.3.11",
    "solid-js": "^1.9.5"
  }
}
